USE STDCALL
USE SYSLIB

_code:
	LDA #0
	LDB #1
	CALL fibonacci

fibonacci:
	CMPB #1000			; compare if B is > than 1000
	JCC *-2				; if not, jump 2 lines ahead
	RET					; else return from routine
	STB switchMemory 	; save B
	ADB A				; B = B + A
	LDA switchMemory	; A = last status of B
	CALL fibonacci

switchMemory: WORD 0 

